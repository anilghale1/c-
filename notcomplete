//obj=obj+obj(calsls overloading)
//    right side parameter
//    left side real value

//wap to overload a function name power which calculates a^b or a+b^c if two or three arguments are passes;
#include<iostream>
using namespace std;

 class Power
 {
 	int a,b,c;
 	public:
 		void setVAlue(int r,int s)
 		{
 			a=r;
 			b=s;
 			
		 }
		 void setValue1(int x,int y,int z)
		  {
		  	 a=x;
		  	 b=y;
		  	 c=z;
		  }
 };
 
 int main()
 {
 	Power obj;
 	int m,n,p;
 	cout<<"Enter value for a,b and c"<<endl;
 	cin>>m>>n>>p;
 	
 	
 	
 	return 0;
 }
