//wap to do operator overloading on unary symbol ++

#include<iostream>
 using namespace std;
 
 class count
 {
 	int value;
 	public:
 		count()
 		{
 			value=5;
		 }
		 
		 void operator ++()
		 {
		 	++value;
		 }
		 
		void display();
	};
	
	void count :: display()
		 {
		 	cout<<"count: "<<value<<endl;
		  } 
	
	int main()
	{
		count count1;
		
		++count1;
		
		count1.display();
		return 0;
	}
